        <div class="container-fluid">
        <div class="row-fluid">
            <div id="header" class="span12">
                <h1>example</h1>
            </div>
        </div>
        <div class="row-fluid">
            <div id="leftcol" class="span8">
            <%= yield %>
            </div>
            <div id="rightcol" class="span4">
                <h4>Load data</h4>
                <button class="btn" id="random">Random grid</button>
                <button class="btn" id="ajaxLoad">AJAX sample.json</button>
                <button class="btn" id="localLoad">Local storage</button>
                <h4>Save data</h4>
                <button class="btn" id="ajaxSave">Save AJAX</button>
                <button class="btn" id="localSave">Save to local storage</button>
                <h4>Edit puzzle</h4>
                <button class="btn" id="toggleCells">Toggle black cells</div>
            </div>
        </div>
        </div><!-- end .container-fluid -->

        <script src="js/lib/jquery-1.9.1.min.js"></script>
        <script src="js/lib/angular-1.0.7.min.js"></script>
        <script src="js/crossword.js"></script>
        <script src="js/events.js"></script>
        <script src="js/navigation.js"></script>
        <script src="js/init.js"></script>

